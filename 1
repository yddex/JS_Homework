var a = 1, b = 1, c, d;
c = ++a; alert(c);           // 2 т.к. это преинкремент, то есть сначала идет операция увелечения переменной a, а затем операция присваивания
d = b++; alert(d);           // 1 т.к. это постинкремент,то есть сначала идет операция присваивания перменной d значения переменной b, а затем операция увелечения переменной a
c = (2+ ++a); alert(c);      // 5 затрудняюсь ответить почему 5. Код отображает после операции 4
d = (2+ b++); alert(d);      // 4  затрудняюсь ответить почему 4. Код отображает после операции 3
alert(a);                    // 3 в коде было 2 операции инкремента с данной переменной
alert(b);                    // 3 в коде было 2 операции инкремента с данной переменной
